    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "eslint:check": "eslint '*.js' 'src/**/*.js'",
    "eslint:fix": "eslint '*.js' 'src/**/*.js' --fix",
